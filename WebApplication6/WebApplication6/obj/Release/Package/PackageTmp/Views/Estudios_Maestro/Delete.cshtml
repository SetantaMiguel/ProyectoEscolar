@model WebApplication6.Models.Estudios_Maestro

@{
    ViewBag.Title = "Delete";
}
<div class="container">
    <h3>Esta seguro de eliminar este Estudio de Maestro?</h3>
    <div>
        <hr />
        <dl class="dl-horizontal">

            <dt>
                @Html.DisplayNameFor(model => model.Str_Tipo_Estudio)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Str_Tipo_Estudio)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Str_Nombre_Estudio)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Str_Nombre_Estudio)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Str_Centro_Estudio)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Str_Centro_Estudio)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Bl_Estado_Estudio)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Bl_Estado_Estudio)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Fch_Finalizacion)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Fch_Finalizacion)
            </dd>

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Borrar" class="btn btn-danger" /> |
                @Html.ActionLink("Regresar", "Index", new { id = Model.Id_Empleado }, new { @class = "btn btn-dark" })
            </div>
        }
    </div>
</div>
